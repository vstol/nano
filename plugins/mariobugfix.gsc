init( modVersion )
{	
	for(;;)
	{
		level waittill( "player_killed", owned, eInflictor, killer, iDamage, sMeansOfDeath, sWeapon, vDir, sHitLoc );
		if(isdefined(owned) && isdefined(killer) && isDefined(level.activ) && isPlayer(owned) && isPlayer(killer))
		{
			if(level.activ == owned && killer.origin[0] >= 2650)
				kick( killer getEntityNumber());
		}
	}
}